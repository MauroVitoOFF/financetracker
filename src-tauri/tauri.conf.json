{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "financetracker",
  "version": "0.1.0",
  "identifier": "com.maurovito.financetracker",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Finance Tracker",
        "width": 1720,
        "height": 880,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDQxMTM4MjQ2RjJBMzRCNjYKUldSbVM2UHlSb0lUUVVZcHdhNDhVTFJrZ2lldm85U3lPWlg4Ukh3eE9zaTZHK0toWU1TRXZseGsK",
      "endpoint": [
        "https://github.com/MauroVitoOFF/financetracker/releases/latest/download/latest.json"
      ]
    }
  }
}
